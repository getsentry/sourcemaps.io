{
  "extends": [
    "../.eslintrc.json",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "globals": {
    "React": false
  },
  "rules": {
     "react/jsx-uses-vars": 1,
     "arrow-parens": ["error", "as-needed"]
  }
}
